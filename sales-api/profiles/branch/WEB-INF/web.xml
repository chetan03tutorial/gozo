   		<param-value>classpath:/context/branchContext.xml</param-value>
   	</context-param>
   	
    <context-param>
        <description>This should be the key for the configuration cache instance.</description>
        <param-name>com.lloydstsb.ea.KEY_CONFIG_MAP</param-name>
        <param-value>CONFIG_MAP</param-value>
    </context-param>
    <context-param>
        <description>
            This should be the namespace for the configuration cache instance.
        </description>
        <param-name>com.lloydstsb.ea.NAMESPACE_CONFIG_MAP</param-name>
        <param-value>java:comp/env/dmap/UIConfigCache_products</param-value>
    </context-param>
    <context-param>
        <description>
            This should be the path for the configuration files.
        </description>
        <param-name>com.lloydstsb.ea.CONFIG_PATH</param-name>
        <param-value>config</param-value>
    </context-param>
    <context-param>
        <description>
            This should be the prefix for the configuration files.
        </description>
        <param-name>com.lloydstsb.ea.CONFIG_PREFIX</param-name>
        <param-value>config</param-value>
    </context-param>
    <context-param>
        <description>
            This should be the extension for the configuration files to be loaded.
        </description>
        <param-name>com.lloydstsb.ea.CONFIG_EXTN</param-name>
        <param-value>xml</param-value>
    </context-param>
    <context-param>
        <description>
            This should be the digester parsing rules filename for the configuration loader.
        </description>
        <param-name>com.lloydstsb.ea.CONFIG_PARSER_RULES</param-name>
        <param-value>config-parser-rules.xml</param-value>
    </context-param>
    <context-param>
        <param-name>archetypes</param-name>
        <param-value>
            /WEB-INF/archetypes.xml
        </param-value>
    </context-param>
     <session-config>
        <session-timeout>20</session-timeout>
    </session-config>
    <listener>
        <listener-class>
            com.lloydstsb.ea.config.servlet.listener.ConfigurationListener
