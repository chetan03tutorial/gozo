    </filter-mapping>

    <filter>
		<filter-name>RestfulUrlMethodFilter</filter-name>
		<filter-class>com.lbg.ib.api.sales.docupload.filter.RestfulUrlMethodFilter
		</filter-class>
	</filter> 
	<filter-mapping>
		<filter-name>RestfulUrlMethodFilter</filter-name>
		<url-pattern>/docUpload/*</url-pattern>
	</filter-mapping>
    <filter-mapping>
        <filter-name>RestfulUrlMethodFilter</filter-name>
        <url-pattern>/account/genDocAndSave</url-pattern>
    </filter-mapping>
	
	<filter>
		<description>DocUploadServiceFilter</description>
		<display-name>DocUploadServiceFilter</display-name>
		<filter-name>docUploadServiceFilter</filter-name>
		<filter-class>org.springframework.web.filter.DelegatingFilterProxy
		</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>docUploadServiceFilter</filter-name>
		<url-pattern>/docUpload/*</url-pattern>
	</filter-mapping>
    <filter-mapping>
        <filter-name>docUploadServiceFilter</filter-name>
        <url-pattern>/account/genDocAndSave</url-pattern>
    </filter-mapping>
	
	<filter>
		<description>AuthenticateCustomerFilter</description>
		<display-name>AuthenticateCustomerFilter</display-name>
		<filter-name>authenticateCustomerFilter</filter-name>
		<filter-class>org.springframework.web.filter.DelegatingFilterProxy
		</filter-class>
	</filter>
	<!-- 
	<filter-mapping>
		<filter-name>tokenAuthorisationFilter</filter-name>
		<url-pattern>/v1/*</url-pattern>
	</filter-mapping>
	 -->
	<filter-mapping>
		<filter-name>authenticateCustomerFilter</filter-name>
		<url-pattern>/docUpload/*</url-pattern>
	</filter-mapping>
    <filter-mapping>
        <filter-name>authenticateCustomerFilter</filter-name>
        <url-pattern>/account/genDocAndSave</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>FontServlet</servlet-name>
        <servlet-class>
            com.lbg.ib.api.sales.common.servlet.FontServlet
        </servlet-class>
        <init-param>
            <param-name>cacheMaxAge</param-name>
            <param-value>86400</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>FontServlet</servlet-name>
        <url-pattern>/fonts/*</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CXFServlet</servlet-name>
        <servlet-class>
            org.apache.cxf.transport.servlet.CXFServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>CXFServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>

    <security-constraint>
        <display-name>primary auth</display-name>
        <web-resource-collection>
            <web-resource-name>primary auth</web-resource-name>
            <url-pattern>/*</url-pattern>
            <http-method>GET</http-method>
            <http-method>POST</http-method>
       </web-resource-collection>
        <auth-constraint>
            <description>login auth</description>
            <role-name>LOGIN_AUTH</role-name>
        </auth-constraint>
    </security-constraint>
    <security-constraint>
        <display-name>secondary auth</display-name>
        <web-resource-collection>
            <web-resource-name>secondary auth</web-resource-name>
            <url-pattern>/*</url-pattern>
            <http-method>GET</http-method>
            <http-method>POST</http-method>
        </web-resource-collection>
        <auth-constraint>
            <description>secondary auth</description>
            <role-name>FULL_AUTH</role-name>
        </auth-constraint>
    </security-constraint>
    
	<security-role id="SecurityRole_LOGIN_AUTH">
        <role-name>LOGIN_AUTH</role-name>
	</security-role>
	<security-role id="SecurityRole_FULL_AUTH">
        <role-name>FULL_AUTH</role-name>
	</security-role>	
	
