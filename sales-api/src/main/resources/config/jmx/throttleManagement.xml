<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MBean SYSTEM "MbeanDescriptor.dtd">
<MBean type="ThrottleConfigMBean" version="5.0"
	platform="dynamicproxy" description="Logging MBean.">

	<operation name="setControlAttributes" role="operation" type=""
		targetObjectType="objectReference" impact="ACTION"
		proxyInvokeType="multicall">
		<signature>
			<parameter name="someAttributes"
				description="The list of attributes for this EA component"
				type="java.lang.Object" />
		</signature>
	</operation>

	<operation name="getThrottleThreshold" role="operation"
		type="java.lang.String" targetObjectType="objectReference"
		impact="INFO" proxyInvokeType="multicall">
		<signature>
			<parameter name="" description="" type="" />
		</signature>
	</operation>

	<operation name="getThrottleFactor" role="operation"
		type="java.lang.String" targetObjectType="objectReference"
		impact="INFO" proxyInvokeType="multicall">
		<signature>
			<parameter name="" description="" type="" />
		</signature>
	</operation>	
	
	<operation name="setThrottleThreshold" role="operation"
		type="java.lang.String" targetObjectType="objectReference"
		impact="ACTION" proxyInvokeType="multicall">
		<signature>
			<parameter name="aThrottleThreshold" description="New value"
				type="java.lang.Object" />
			<parameter name="anUpdateType"
				description="Managed or Direct Update." type="java.lang.Object" />
		</signature>
	</operation>

	<operation name="setThrottleFactor" role="operation"
		type="java.lang.String" targetObjectType="objectReference"
		impact="ACTION" proxyInvokeType="multicall">
		<signature>
			<parameter name="aThrottleFactor" description="New value"
				type="java.lang.Object" />
			<parameter name="anUpdateType"
				description="Managed or Direct Update." type="java.lang.Object" />
		</signature>
	</operation>

	
	
	<operation name="getXMLConfig" role="operation"
		type="java.lang.String" targetObjectType="objectReference"
		impact="INFO" proxyInvokeType="multicall">
		<signature>
			<parameter name="" description="" type="" />
		</signature>
	</operation>

	<operation name="updateXMLConfig" role="operation" type=""
		targetObjectType="objectReference" impact="ACTION"
		proxyInvokeType="multicall">
		<signature>
			<parameter name="anXMLConfig" description="the new XML"
				type="java.lang.Object" />
			<parameter name="anUpdateType"
				description="Managed or Direct Update." type="java.lang.Object" />
		</signature>
	</operation>

	<notification name="applicationJMXBroadcaster" severity="3">
		<notificationType>JMX.mbean.registered</notificationType>
	</notification>
</MBean>
